bool check(int a, int N) {
    return ((a < N) && (a >= 0));
}

int main(){
    if(check(1,2)){
        print("true");
    } else {
        print("false");
    }
    if(check(-1,1)){
        print("true");
    } else {
        print("false");
    }
    if(check(1,-1)){
        print("true");
    } else {
        print("false");
    }
    if(check(-1,-1)){
        print("true");
    } else {
        print("false");
    }

    return 0;
}